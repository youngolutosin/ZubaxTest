 
TEST(BigInteger, BigIntegerMultiplication) 
    { 
    ASSERT_EQ("25526350", multiply("10030", "2545"));
    ASSERT_EQ("4777681040496", multiply("181818", "26277272"));
    ASSERT_EQ("838383883", multiply("26277272", "4777681040496"));
    ASSERT_EQ("39793007149945436100475", multiply("151525352525", "262616166119"));
    ASSERT_EQ("1664528000760286702952579179595709221605411669348936629786", multiply("82934479729247174132156258162","2007039781517131078583025653"));
    }


 ASSERT_EQ("39794490416508552682075 ", findSum("39793007149945436100475", "1483266563116581600"));
    ASSERT_EQ("1664528000760286702952579179821677915367868112822353247564", findSum("1664528000760286702952579179595709221605411669348936629786", "225968693762456443473416617778"));





 
TEST(BigInteger, Subtraction) 
    {
    BigInterger Sub;
    string value3 = Sub.findDiff("97877272727272727727", "12977717272727272772"));
    string value4 = Sub.findDiff("10081818181818187171771717171", "10000007717171717171771717171");
    ASSERT_EQ("84899555454545454955", value3);
    ASSERT_EQ("00081810464646470000000000000",value4);
    }

